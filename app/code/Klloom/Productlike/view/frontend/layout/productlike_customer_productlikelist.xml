<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>Photos I've Liked</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Photos I've Liked</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Klloom\Productlike\Block\Countlink" name="productlike_customer_productlikelist" template="Klloom_Productlike::account/productlike.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>